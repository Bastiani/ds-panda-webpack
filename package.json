{
  "name": "@rafacdb/ds-panda-webpack",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.bundle.js",
  "unpkg": "lib/index.bundle.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/Bastiani/ds-panda-webpack#readme",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn build:lib && yarn build:min && yarn build:types",
    "build:dev": "yarn build:types && NODE_ENV=development webpack --watch --progress",
    "build:lib": "NODE_ENV=production webpack",
    "build:min": "cd lib && uglifyjs --compress --mangle --source-map --comments -o index.bundle.min.js -- index.bundle.js && gzip index.bundle.min.js -c > index.bundle.min.js.gz",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir lib",
    "start": "webpack serve -c webpack.config.js",
    "publish": "npm publish --access=public",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.13.0",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "babel-loader": "^8.2.2",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "path": "^0.12.7",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.1.2",
    "uglify-js": "^3.13.6",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
